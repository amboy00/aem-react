    "react-ga": "^2.1.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-scroll": "^0.2.0",
    "react-sticky": "^5.0.4",
    "redux": "^3.1.0",
    "redux-form": "^6.0.0-rc.4",
